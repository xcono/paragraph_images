{%
set classes = [
'paragraph',
paragraph.bundle|clean_class,
(paragraph.bundle|clean_class) ~ '--' ~ style,
view_mode ? 'paragraph--view-mode-' ~ view_mode|clean_class,
]
%}

<div{{ attributes.addClass(classes) }}>
    {{ content }}
</div>


